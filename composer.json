{
    "name": "centrust/datefield",
    "description": "A Laravel Nova field.",
    "keywords": [
        "laravel",
        "nova"
    ],
    "license": "MIT",
    "require": {
        "php": "^7.3|^8.0|^8.1"
    },
    "autoload": {
        "psr-4": {
            "Centrust\\Datefield\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Centrust\\Datefield\\FieldServiceProvider"
            ]
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
